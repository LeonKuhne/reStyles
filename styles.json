{
  "www.examcompass.com": {
    "body": {
      "background": "black"
    },
    "#sidebar": {
      "display": "none"
    },
    "#gk-content": {
      "width": "100%"
    },
    "#adminForm": {
      "min-height": "750px"
    },
    "#gk-breadcrumb": {
      "display": "none"
    }
  },
  "calendar.google.com": {
    "body > div, .GENA3c, header, div[role=grid]": {
      "background": "black"
    },
    "body > div div": {
      "color": "magenta"
    },
    "body > div span": {
      "color": "#4F4"
    },
    "div[role=button]": {
      "filter": "saturate(2) brightness(0.8)"
    }
  },
  "leonkuhne.com": {
    "body": {
      "background": "black",
      "color": "magenta"
    },
    "img": {
      "box-shadow": "magenta 0 0 10px"
    },
    "#headshot": {
      "border": "cyan solid 10px"
    }
  }
}
